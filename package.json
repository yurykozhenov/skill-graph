  },
  "devDependencies": {
    "@types/jest": "24.0.18",
    "@types/lodash": "^4.14.146",
    "@types/node": "12.7.12",
    "@types/react": "16.9.5",
    "@types/react-dom": "16.9.1",
    "prettier": "1.18.2",
    "react-scripts": "3.2.0",
    "typescript": "3.6.4"
  }
